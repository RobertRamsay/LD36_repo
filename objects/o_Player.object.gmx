        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controls - Driving Phys
if keyboard_check(ord('W'))
    {
    speed += 1;
    
    if (speed &gt; maxSpeed + 5 ) { speed = maxSpeed + 5; }
    }
    
if keyboard_check(ord('S'))
    {
        ///Brakes
    
    speed -= 0.5
    
    if (speed &lt; 0) {
        speed = 0;
    }
    }
//Controls
if ( keyboard_check_pressed(vk_left) or keyboard_check_pressed(vk_right) or keyboard_check_pressed(ord('A')) or keyboard_check_pressed(ord('D'))) 
    {timeTurning=0;}

if ( keyboard_check(vk_left) or keyboard_check(ord('A')) ) {
    image_index += 1;
    timeTurning+=1;
    if timeTurning=7
        {
        if !audio_is_playing(sou_screech)
            {audio_play_sound(sou_screech,1,0)}
        }
}
if (keyboard_check(vk_right) or keyboard_check(ord('D'))) {
    image_index -= 1
    timeTurning+=1;
    if timeTurning=7
        {
        if !audio_is_playing(sou_screech)
            {audio_play_sound(sou_screech,1,0)}
        }
}

PIAngle = myDir.image_angle;

motion_set(PIAngle, speed);

if ( keyboard_check_pressed(ord('Z')) or mouse_check_button_pressed(mb_left)) {
    // hit left
    animstate=clubleft
    }
    
if ( keyboard_check_pressed(ord('X'))or mouse_check_button_pressed(mb_right) ) {
    // hit right
    animstate=clubright
    }
    
if animstate=clubright and done=0
    {
    sprite_index=s_aimClub_R
    if !audio_is_playing(sou_hitWithClub) {audio_play_sound(sou_swipe,1,0)}
    alarm[1]=10;done=1
    }
    
if animstate=clubleft and done=0
    {
    sprite_index=s_aimClub_L
    if !audio_is_playing(sou_hitWithClub) {audio_play_sound(sou_swipe,1,0)}
    alarm[2]=10;done=1
    }

</string>
          </argument>
        </arguments>
      </action>
